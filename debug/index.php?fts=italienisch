emInitFlyoutMenu();


/* Überschriften neu zentrieren, wenn Ressortliste zu lang - Bernhard Schinkitz */
if((jQuery(".em_ressort_breadcrumb").length) && (jQuery("#em_cnt_artikel").length == 0)) {
jQuery(".em_ressort_headline").each(function() {

var _this = jQuery(this);
var _thisTestChild = _this.has(".em_ressort_breadcrumb li");

if(_thisTestChild.length) {

// cache
var _thisChild = _this.find(".em_ressort_breadcrumb");
var _thisHeadline = _this.find("h2");
var _thisHeadlineOffset = _thisHeadline.offset().left;

// Gesamtlänge der Navigation bestimmen
var intNavLengthAll = _thisChild.width() + _thisChild.offset().left;

if(intNavLengthAll > _thisHeadline.offset().left) {

if(jQuery.browser.msie) {
_this.css({
"text-align" : "left"
});

_thisHeadline.css({
"position" : "absolute",
"display" : "inline"
});
} else {
_this.css({
"text-align" : "left"
});
}

_thisHeadline.css({
"margin-left" : Math.round(_thisChild.width() + ((_this.width() - _thisChild.width()) / 2) - _thisHeadline.width() - parseInt(_thisHeadline.css("padding-left")) - parseInt(_thisHeadline.css("padding-right"))) + "px"
});
}
}
});
}
/* ]]> */
</script>
